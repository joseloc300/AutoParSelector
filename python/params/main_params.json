{
  "read_data_prefixes": ["poly"],
  "sampling_params": {
    "train_ratio": 0.8,
    "block_size": 10,
    "stratify": true,
    "randomize": true,
    "r_seed": null,
    "r_shuffles": null
  },
  "data_analysis": {
    "pre_data_processing": false,
    "post_data_processing": true,
    "data_correlation": {
      "enabled": true,
      "graphical_results": false,
      "top_x_pos_correlation_target": 10,
      "top_x_neg_correlation_target": 10,
      "top_x_abs_correlation_target": 20
    }
  },
  "data_processing": {
    "threshold_target": {
      "enabled": true,
      "threshold": 0.25
    },
    "handle_invalid_inputs": true,
    "scale_data_algorithm": 2,
    "feature_selection_params": {
      "enabled": true,
      "algorithm": 0,
      "variance_threshold": 0.05,
      "target_correlation": {
        "use_threshold": false,
        "mode": "abs",
        "target_correlation_threshold": 0.02,
        "top_x_correlation": 20
      }
    },
    "regression_smote": true
  },
  "create_models": {
    "regression": {
      "enabled": false,
      "ordinary_least_squares": true,
      "lasso": false,
      "elastic_net": false,
      "svr": false,
      "ridge_regression": false,
      "decision_tree_regressor": true,
      "neural_network_sklearn": false
    },
    "classification": {
      "enabled": true,
      "svc": false,
      "kn_classifier": true
    }
  },
  "make_prediction": {
    "enabled": true,
    "model_name": "kn_classifier",
    "is_classification": true,
    "model_folder_path": "./models/classification/kn_classifier"
  }
}